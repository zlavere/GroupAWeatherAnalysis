<Application
    x:Class="WeatherDataAnalysis.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:WeatherDataAnalysis.Converter">
    <Application.Resources>
        <converters:OrdinalDateConverter x:Key="OrdinalDateConverter" />
        <converters:DatePickerConverter x:Key="DatePickerConverter" />
        <converters:StringInputToInt x:Key="StringToInt" />
        <converters:HighTempConverter x:Key="HighTempConverter" />
        <converters:LowTempConverter x:Key="LowTempConverter" />
        <converters:MostPrecipitationConverter x:Key="MostPrecipitationConverter" />
        <converters:CollectionGroupAndDaysInYearDataConverter x:Key="DaysInYearDataConverter" />
        <converters:CollectionGroupAndDaysInMonthDataConverter x:Key="DaysInMonthDataConverter" />
        <converters:CollectionHistogramGenerator x:Key="HistogramConverter" />
    </Application.Resources>

</Application>