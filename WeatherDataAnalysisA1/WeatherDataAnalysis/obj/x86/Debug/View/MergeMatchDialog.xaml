<ContentDialog x:ConnectionId='1'
    x:Class="WeatherDataAnalysis.View.MergeMatchDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="Keep or Replace"
    PrimaryButtonText="Replace"
    SecondaryButtonText="Keep"
                                  
                                 >

    <Grid Margin="0,0,12,-4">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150*"/>

            <ColumnDefinition Width="150*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <TextBlock x:ConnectionId='2' x:Name="activeDateText" HorizontalAlignment="Left" Margin="0,0,0,0" Text="Date:" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column ="0" Grid.Row="1" />
        <TextBlock x:ConnectionId='3' x:Name="newDateText" HorizontalAlignment="Left" Margin="0,0,0,0" Text="Date:" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column ="1" Grid.Row="1"/>
        <TextBlock x:ConnectionId='4' x:Name="newHighTempText" HorizontalAlignment="Left" Margin="0,0,0,0" Text="High:" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column ="1" Grid.Row="2"/>
        <TextBlock x:ConnectionId='5' x:Name="newLowTempText" HorizontalAlignment="Left" Margin="0,0,0,0" Text="Low" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column ="1" Grid.Row="3" />
        <TextBlock x:ConnectionId='6' x:Name="activeHighTempText" HorizontalAlignment="Left" Margin="0,0,0,0" Text="High:" TextWrapping="Wrap" VerticalAlignment="Top"  Grid.Column ="0" Grid.Row="2" />
        <TextBlock x:ConnectionId='7' x:Name="activeLowTempText" HorizontalAlignment="Left" Margin="0,0,0,0" Text="Low:" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Column ="0" Grid.Row="3" />
        <TextBlock HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" Text="Current Information" Grid.Column ="0" Grid.Row="0"/>
        <TextBlock HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" Text="New Information" Grid.Column="1" Grid.Row="0" />
    </Grid>
</ContentDialog>

